<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:importantForAutofill="noExcludeDescendants"
    android:padding="4dp">

  <TextView
      android:id="@+id/subject_label"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:labelFor="@id/subject_edit"
      android:text="@string/subject_edit_label"
      app:layout_constraintBaseline_toBaselineOf="@id/subject_edit"
      app:layout_constraintStart_toStartOf="parent" />
  <EditText
      android:id="@+id/subject_edit"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:inputType="text"
      android:minHeight="48dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@id/subject_label"
      app:layout_constraintTop_toTopOf="parent" />

  <com.teratail.q_3mipiypmm6w99c.ElementView
      android:id="@+id/element_1"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="10dp"
      android:background="#f0f0ff"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/subject_edit" />

  <com.teratail.q_3mipiypmm6w99c.ElementView
      android:id="@+id/element_2"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:background="#f0fff0"
      app:layout_constraintEnd_toEndOf="@id/element_1"
      app:layout_constraintStart_toStartOf="@id/element_1"
      app:layout_constraintTop_toBottomOf="@id/element_1" />

  <com.teratail.q_3mipiypmm6w99c.ElementView
      android:id="@+id/element_3"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:background="#f0ffff"
      app:layout_constraintEnd_toEndOf="@id/element_2"
      app:layout_constraintStart_toStartOf="@id/element_2"
      app:layout_constraintTop_toBottomOf="@id/element_2" />

  <com.teratail.q_3mipiypmm6w99c.ElementView
      android:id="@+id/element_4"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:background="#fff0f0"
      app:layout_constraintEnd_toEndOf="@id/element_3"
      app:layout_constraintStart_toStartOf="@id/element_3"
      app:layout_constraintTop_toBottomOf="@id/element_3" />

  <com.teratail.q_3mipiypmm6w99c.ElementView
      android:id="@+id/element_5"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:background="#f0f0f0"
      app:layout_constraintEnd_toEndOf="@id/element_4"
      app:layout_constraintStart_toStartOf="@id/element_4"
      app:layout_constraintTop_toBottomOf="@id/element_4" />

</androidx.constraintlayout.widget.ConstraintLayout>